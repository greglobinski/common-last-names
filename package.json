{
    "name": "common-last-names",
    "version": "0.0.0-semantically-released",
    "description": "Get most common last names",
    "main": "dist/index.js",
    "scripts": {
        "commit": "git-cz",
        "prebuild": "rimraf dist",
        "build": "npm-run-all --parallel build:*",
        "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
        "build:umd": "webpack --output-filename index.umd.js",
        "build:umd.min": "webpack --output-filename index.umd.min.js -p",
        "test": "cross-env NODE_ENV=test nyc mocha",
        "watch:test": "mocha --watch",
        "lint": "eslint src",
        "report-coverage": "cat ./coverage/lcov.info | codecov",
        "validate": "cross-env NO_UPDATE_NOTIFIER=true npm-run-all --parallel test lint build",
        "travis-deploy-once": "travis-deploy-once",
        "semantic-release": "semantic-release",
        "commitmsg": "commitlint -E GIT_PARAMS"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/greglobinski/common-last-names.git"
    },
    "keywords": [
        "random",
        "last names"
    ],
    "files": [
        "dist"
    ],
    "author": "greg lobinski <greglobinski@gmail.com> (https://www.greglobinski.com/)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/greglobinski/common-last-names/issues"
    },
    "homepage": "https:/github.com/greglobinski/common-last-names#readme",
    "dependencies": {
        "unique-random-array": "1.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.0.0",
        "@commitlint/config-conventional": "^7.0.1",
        "babel-cli": "6.11.4",
        "babel-loader": "6.2.4",
        "babel-plugin-istanbul": "1.0.3",
        "babel-preset-es2015": "6.9.0",
        "babel-register": "6.11.6",
        "chai": "3.5.0",
        "codecov": "1.0.1",
        "commitizen": "^2.10.1",
        "cross-env": "2.0.0",
        "cz-conventional-changelog": "^2.1.0",
        "eslint": "3.2.0",
        "eslint-config-kentcdodds": "^9.0.0",
        "husky": "^1.0.0-rc.9",
        "json-loader": "0.5.4",
        "mocha": "3.0.0",
        "npm-run-all": "2.3.0",
        "nyc": "7.1.0",
        "rimraf": "2.5.4",
        "semantic-release": "^15.6.0",
        "travis-deploy-once": "^5.0.0",
        "webpack": "1.13.1"
    },
    "nyc": {
        "all": true,
        "check-coverage": true,
        "branches": 100,
        "function": 100,
        "lines": 100,
        "statements": 100,
        "reporter": [
            "text",
            "lcov"
        ],
        "include": [
            "src"
        ],
        "sourceMap": false,
        "instrument": false,
        "require": [
            "babel-register"
        ]
    },
    "babel": {
        "presets": [
            "es2015"
        ],
        "env": {
            "test": {
                "plugins": [
                    "istanbul"
                ]
            }
        }
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run validate",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
